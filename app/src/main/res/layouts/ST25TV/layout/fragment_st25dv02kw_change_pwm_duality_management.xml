<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".type5.st25dv02kw.ST25DV02KWChangePwmDualityManagementActivity">

    <TextView
        android:id="@+id/selectDualityManangement"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/ouputTrimmingRadioGroup"
        android:text="@string/select_pwm_duality_management"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:textStyle="bold"
        android:typeface="serif"/>

    <RadioGroup
        android:id="@+id/selectDualityMgtRadioGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/selectDualityManangement"
        android:orientation="vertical"
        android:layout_marginTop="10dp" >


        <RadioButton
            android:id="@+id/fullDuplexRadioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:checked="true"
            android:gravity="left"
            android:text="@string/full_duplex"
            android:textColor="@color/st_dark_blue"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/pwmSetInHzWhileRfCmdRadioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:checked="true"
            android:gravity="left"
            android:text="@string/pwm_set_in_hz_while_rf_cmd"
            android:textColor="@color/st_dark_blue"
            android:textStyle="bold" />


        <RadioButton
            android:id="@+id/pwmOneQuarterFullPowerWhileRfCmdRadioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:checked="false"
            android:gravity="left"
            android:text="@string/one_quarter_full_power_while_rf_cmd"
            android:textColor="@color/st_dark_blue"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/pwmFreqReducedRadioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:checked="false"
            android:gravity="left"
            android:text="@string/freq_reduced"
            android:textColor="@color/st_dark_blue"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/pwmFreqReducedOneQuarterFullPowerWhileRfCmdRadioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:checked="false"
            android:gravity="left"
            android:text="@string/freq_reduced_and_one_quarter_full_power_while_rf_cmd"
            android:textColor="@color/st_dark_blue"
            android:textStyle="bold" />

    </RadioGroup>

    <Button
        android:id="@+id/updateTagButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/update_tag"
        android:layout_below="@+id/selectDualityMgtRadioGroup"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp" />


</RelativeLayout>
